# ========= Frontend (Vite) =========
# Entorno y build metadata
VITE_APP_ENV=development
VITE_APP_NAME=agents-frontend
VITE_APP_URL=http://localhost:5173
VITE_RELEASE=local-dev-sha

# Supabase (cliente web)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe (public key para inicializar Stripe.js)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_replace_me

# Seguridad / Captcha (Turnstile sitio)
VITE_TURNSTILE_SITE_KEY=1x00000000000000000000AA

# Notificaciones frontend (valores dummy seguros)
VITE_SENDGRID_KEY=public-sendgrid-placeholder
VITE_TWILIO_AUTH_TOKEN=twilio-test-token

# Observabilidad web (Sentry + métricas UX)
VITE_SENTRY_DSN=https://examplePublicKey.ingest.us.sentry.io/000000
VITE_SENTRY_CDN=https://js.sentry-cdn.com/example.js
VITE_SENTRY_MONITOR_SLUG=agenthub-frontend
VITE_SENTRY_TRACES_SAMPLE_RATE=0.3
VITE_SENTRY_PROFILES_SAMPLE_RATE=0.1
VITE_SENTRY_ERROR_SAMPLE_RATE=1.0
VITE_SENTRY_REPLAYS_SAMPLE_RATE=0.0
VITE_SENTRY_REPLAYS_ON_ERROR_SAMPLE_RATE=1.0
VITE_APDEX_THRESHOLD=0.3

# ========= Edge Functions (Supabase) =========
# Acceso a Supabase con rol de servicio
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=supabase-service-role-key

# Stripe (backend)
STRIPE_SECRET_KEY=sk_test_replace_me
STRIPE_WEBHOOK_SECRET=whsec_replace_me
STRIPE_PRICE_BASIC=price_basic_id
STRIPE_PRICE_ULTRA=price_ultra_id

# IA / embeddings
OPENAI_API_KEY=sk-openai-placeholder

# Emails transaccionales
SENDGRID_API_KEY=SG.example-api-key
FROM_EMAIL=noreply@example.com

# Seguridad / 2FA y captcha
TURNSTILE_SECRET_KEY=1x0000000000000000000000000000000AA
TOTP_ENCRYPTION_KEY=base64-32-byte-key

# Cloudflare Workers (despliegue de agentes)
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_API_TOKEN=cf-api-token-with-workers-perms

# Observabilidad Edge (Sentry + métricas opcionales)
SENTRY_DSN=https://examplePublicKey.ingest.us.sentry.io/000000
SENTRY_TRACES_SAMPLE_RATE=0.2
SENTRY_PROFILES_SAMPLE_RATE=0.05
SENTRY_ERROR_SAMPLE_RATE=0.5
SENTRY_DEBUG=false
ENVIRONMENT=development
RELEASE=local-dev
PROM_PUSHGATEWAY_URL=https://pushgateway.example.com
HOSTNAME=edge-function
